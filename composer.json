{
    "name": "marceloeatworld/falai-php",
    "description": "Modern PHP client for FAL.AI API with queue processing, streaming, webhooks, and automatic array normalization",
    "keywords": ["fal-ai", "fal.ai", "php", "api-client", "ai", "llm", "image-generation", "video-generation", "audio", "queue", "streaming", "webhooks"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/marceloeatworld/falai-php",
    "support": {
        "issues": "https://github.com/marceloeatworld/falai-php/issues",
        "source": "https://github.com/marceloeatworld/falai-php"
    },
    "authors": [
        {
            "name": "Marcelo Pereira",
            "email": "diagngo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "hosmelq/sse-saloon": "^0.1.0",
        "saloonphp/saloon": "^3.14"
    },
    "suggest": {
        "ext-sodium": "Required for webhook signature verification",
        "psr/simple-cache": "For caching webhook public keys and improved performance"
    },
    "autoload": {
        "psr-4": {
            "MarceloEatWorld\\FalAI\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
