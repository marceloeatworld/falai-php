{
    "name": "marceloeatworld/falai-php",
    "description": "A comprehensive PHP client for the FAL.AI API with queue support, webhooks, and synchronous requests",
    "keywords": ["fal-ai", "fal.ai", "php", "api-client", "image-generation", "ai", "machine-learning", "queue", "webhooks"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/marceloeatworld/falai-php",
    "support": {
        "issues": "https://github.com/marceloeatworld/falai-php/issues",
        "source": "https://github.com/marceloeatworld/falai-php"
    },
    "authors": [
        {
            "name": "Marcelo Pereira",
            "email": "diagngo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "saloonphp/saloon": "^3.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "MarceloEatWorld\\FalAI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarceloEatWorld\\FalAI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-unit": "phpunit tests/FalAITest.php",
        "test-integration": "phpunit tests/IntegrationTest.php",
        "test-all": "php tests/run-all-tests.php",
        "test-quick": "php tests/Integration/quick-validation.php",
        "test-working": "php tests/Integration/test-100-percent-working.php"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}